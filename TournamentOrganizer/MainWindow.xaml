<Window x:Class="TournamentOrganizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:tabData ="clr-namespace:TournamentOrganizer.UI.Tabs"
        mc:Ignorable="d"
        Title="Организатор турниров" Height="450" Width="800">
    <Grid>
        <TabControl x:Name="MainTabControl">
            <tabData:TabItemStart></tabData:TabItemStart>
            <tabData:TabItemParticipants></tabData:TabItemParticipants>
            <tabData:TabItemGame></tabData:TabItemGame>
            
        </TabControl>
    </Grid>
</Window>
